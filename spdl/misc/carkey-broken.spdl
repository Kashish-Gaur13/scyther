const pk: Function;
secret sk: Function;
inversekeys (pk,sk);

protocol carkeybroken(I,R)
{
	role I
	{
		const ni: Nonce;

		send_1(I,R, {ni}sk(I) );
	}	
	
	role R
	{
		var ni: Nonce;

		read_1(I,R, {ni}sk(I) );
		claim_2(R,Nisynch);
	}
}

const Alice,Bob,Eve: Agent;

untrusted Eve;
const nc: Nonce;
compromised sk(Eve);

run carkeybroken.I(Agent,Agent);
run carkeybroken.R(Agent,Agent);
run carkeybroken.I(Agent,Agent);
run carkeybroken.R(Agent,Agent);
