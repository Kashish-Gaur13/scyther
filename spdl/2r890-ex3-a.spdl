		var nx: Nonce;

		read_1(I,X, nx );
		send_2(X,I, { nx,I }sk(X) );
	}

	role Y
	{
		var ny: Nonce;

		read_3(I,Y, ny );
		send_4(Y,I, { I,ny }sk(Y) );
	}
}

