		send_1(I,X, nx );
		read_2(X,I, { nx,I }sk(X) );
		send_3(I,Y, ny );
		read_4(Y,I, { I,ny }sk(Y) );

		claim_i1(I,Niagree);
		claim_i2(I,Nisynch);
