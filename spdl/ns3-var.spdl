const pk: Function;
secret sk: Function;
inversekeys (pk,sk);

protocol ns3var(I,R)
{
	role I
	{
		const ni: Nonce;
		var nr: Nonce;

		send_1(I,R, {I,ni}pk(R) );
		read_2(R,I, {ni,nr}pk(I) );
		send_3(I,R, {nr}pk(R) );
		claim_4(I,Secret,ni,nr);
	}	
	
	role R
	{
		var ni: Nonce;
		const nr: Nonce;

		read_1(I,R, {I,ni}pk(R) );
		send_2(R,I, {ni,nr}pk(I) );
		read_3(I,R, {nr}pk(R) );
		claim_5(R,Secret,ni,nr);
	}
}

const Alice,Bob,Eve : Agent;

untrusted Eve;
const nc: Nonce;
compromised sk(Eve);

run ns3var.I(Agent,Agent);
run ns3var.R(Agent,Agent);
